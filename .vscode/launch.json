{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "4800"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "ENV": "dev"
            },
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}